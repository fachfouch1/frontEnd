@model IEnumerable<Model.AdAppointment.Ad>

@{
    ViewBag.Title = "Index";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.price_per_day)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.price_per_month)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.price)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ad_Etat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Governorate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Delegation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.surface)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nbrpieces)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.rooms)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.image)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.piscine)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.taux_reduction)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.price_per_day)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.price_per_month)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AdType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ad_Etat)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Governorate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Delegation)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.surface)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.nbrpieces)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.rooms)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.image)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.piscine)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.taux_reduction)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }

    </table>
</body>
</html>
